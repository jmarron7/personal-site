	--color-surface-400: 88 93 99; /* ⬅ #585d63 */
	--color-surface-500: 16 24 32; /* ⬅ #101820 */
	--color-surface-600: 14 22 29; /* ⬅ #0e161d */
	--color-surface-700: 12 18 24; /* ⬅ #0c1218 */
	--color-surface-800: 10 14 19; /* ⬅ #0a0e13 */
	--color-surface-900: 8 12 16; /* ⬅ #080c10 */
}

