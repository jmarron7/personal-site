	--color-warning-100: 246 248 243; /* ⬅ #f6f8f3 */
	--color-warning-200: 243 247 241; /* ⬅ #f3f7f1 */
	--color-warning-300: 236 242 232; /* ⬅ #ecf2e8 */
	--color-warning-400: 222 232 214; /* ⬅ #dee8d6 */
	--color-warning-500: 208 222 197; /* ⬅ #D0DEC5 */
	--color-warning-600: 187 200 177; /* ⬅ #bbc8b1 */
	--color-warning-700: 156 167 148; /* ⬅ #9ca794 */
	--color-warning-800: 125 133 118; /* ⬅ #7d8576 */
	--color-warning-900: 102 109 97; /* ⬅ #666d61 */
	/* error | #e4002b */
	--color-error-50: 251 217 223; /* ⬅ #fbd9df */
	--color-error-100: 250 204 213; /* ⬅ #faccd5 */
	--color-error-200: 248 191 202; /* ⬅ #f8bfca */
	--color-error-300: 244 153 170; /* ⬅ #f499aa */
	--color-error-400: 236 77 107; /* ⬅ #ec4d6b */
	--color-error-500: 228 0 43; /* ⬅ #e4002b */
	--color-error-600: 205 0 39; /* ⬅ #cd0027 */
	--color-error-700: 171 0 32; /* ⬅ #ab0020 */
